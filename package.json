{
    "name": "@calvinckho/capacitor-contact-picker",
    "version": "1.0.2",
    "description": "Allows users to select a single contact from their device.",
    "main": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "scripts": {
        "build": "npm run clean && tsc",
        "clean": "rimraf ./dist",
        "watch": "tsc --watch",
        "prepare": "npm run build",
        "prepublishOnly": "npm run build"
    },
    "author": "Calvin Ho",
    "license": "MIT",
    "dependencies": {
        "@capacitor/cli": "^2.0.2",
        "@capacitor/core": "latest"
    },
    "devDependencies": {
        "@capacitor/android": "latest",
        "@capacitor/ios": "latest",
        "rimraf": "^3.0.0",
        "typescript": "^3.2.4"
    },
    "files": [
        "dist/",
        "ios/",
        "android/",
        "CalvinckhoCapacitorContactPicker.podspec"
    ],
    "keywords": [
        "capacitor",
        "plugin",
        "native"
    ],
    "capacitor": {
        "ios": {
            "src": "ios"
        },
        "android": {
            "src": "android"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/calvinckho/capacitor-contact-picker"
    },
    "bugs": {
        "url": "https://github.com/calvinckho/capacitor-contact-picker/issues"
    }
